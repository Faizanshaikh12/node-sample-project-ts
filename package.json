{
  "name": "SherBazaar",
  "version": "1.0.0",
  "description": "SherBazaar Admin Application",
  "main": "index.js",
  "engines": {
    "node": "16.15.0"
  },
  "scripts": {
    "watch": "tsc -w",
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.json && npm run-script copy-to-dist",
    "copy-to-dist": "copyfiles --up 1 src/**/*.html src/keys/* build/src",
    "lint": "eslint 'src/**/*.ts'",
    "dev": "npm run && ts-node-dev --respawn -r dotenv/config -r source-map-support/register src/index.ts",
    "start": "node -r dotenv/config build/src/index.js"
  },
  "license": "ISC",
